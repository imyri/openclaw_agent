# Exchange
BINANCE_API_KEY=replace_with_testnet_key
BINANCE_SECRET_KEY=replace_with_testnet_secret
ENABLE_TESTNET=true
EXECUTE_ORDERS=true
TRADING_SYMBOL=BTC/USDT
TRADING_TIMEFRAME=5m
MAX_CANDLES=100

# Telegram (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
TELEGRAM_ALERTS_ENABLED=true
TELEGRAM_ALERTS_INCLUDE_WAIT=false

# Database
POSTGRES_USER=openclaw_admin
POSTGRES_PASSWORD=replace_with_strong_password
POSTGRES_DB=openclaw_state
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@database:5432/${POSTGRES_DB}

# LLM
OLLAMA_BASE_URL=http://ollama-brain:11434
OLLAMA_MODEL=deepseek-r1:7b
OLLAMA_TIMEOUT_SECS=30

# Internal worker -> API bridge
INTERNAL_API_TOKEN=replace_with_internal_token
INTERNAL_API_URL=http://backend:8000/internal/ai-event

# Risk
MAX_DAILY_DRAWDOWN_R=2.0
RISK_PER_TRADE_PERCENT=0.01
MAX_TRADE_DURATION_MINS=30
DEFAULT_ACCOUNT_BALANCE=10000.0
MIN_RR_RATIO=3.0
